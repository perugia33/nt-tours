    "url": "git+https://github.com/perugia33/nt-tours"
  },
  "author": "\"Theresa\"",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/perugia33/nt-tours/issues"
  },
  "homepage": "https://github.com/perugia33/nt-tours#readme",
  "devDependencies": {
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "jquery": "^3.7.1",
    "morgan": "^1.10.0",
    "prettier": "^3.1.0"
